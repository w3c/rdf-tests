      "@id": "#put__initial_state",
      "@type": "mf:GraphStoreProtocolTest",
      "name": "PUT - Initial state",
      "approval": "dawg:Approved",
      "approvedBy": "http://www.w3.org/2009/sparql/meeting/2012-11-20#resolution_3",
      "action": {
        "@type": "ht:Connection",
        "ht:connectionAuthority": "www.example",
        "requests": [
          {
            "@type": "ht:Request",
            "ht:methodName": "PUT",
            "ht:httpVersion": "1.1",
            "ht:absolutePath": "$GRAPHSTORE$/person/1.ttl",
            "headers": [
              {
                "@type": "ht:RequestHeader",
                "ht:fieldName": "Host",
                "ht:fieldValue": "$HOST$",
                "headerElements": [
                  {
                    "@type": "ht:HeaderElement",
                    "ht:elementName": "$HOST$"
                  }
                ]
              },
              {
                "@type": "ht:RequestHeader",
                "ht:fieldName": "Content-Type",
                "ht:fieldValue": "text/turtle; charset=utf-8",
                "headerElements": [
                  {
                    "@type": "ht:HeaderElement",
                    "ht:elementName": "text/turtle",
                    "params": [
                      {
                        "@type": "ht:Parameter",
                        "ht:paramName": "charset",
                        "ht:paramValue": "utf-8"
                      }
                    ]
                  }
                ]
              }
            ],
            "ht:body": {
              "@type": "cnt:ContentAsText",
              "cnt:chars": "\n@prefix foaf: <http://xmlns.com/foaf/0.1/> .\n@prefix v: <http://www.w3.org/2006/vcard/ns#> .\n\n<http://$HOST$/$GRAPHSTORE$/person/1> a foaf:Person;\n    foaf:businessCard [\n        a v:VCard;\n        v:fn \"John Doe\"\n    ].\n",
              "cnt:characterEncoding": "utf-8"
            },
            "ht:resp": {
              "@type": "ht:Response",
              "ht:statusCodeValue": [
                "201"
              ]
            }
          }
        ]
      }
    },
